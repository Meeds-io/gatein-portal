<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" version="2.0"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd
   http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
  <portlet>
    <description xml:lang="EN">How to create an AMD module</description>
    <portlet-name>Sample1Portlet</portlet-name>
    <display-name xml:lang="EN">Create an AMD module</display-name>
    <portlet-class>org.gatein.portlet.amd.Sample1Portlet</portlet-class>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
      <title>Sample 1: How to create an AMD module</title>
    </portlet-info>
  </portlet>

  <portlet>
    <description xml:lang="EN">JQuery Plugin Sample Portlet</description>
    <portlet-name>JQueryPluginPortlet</portlet-name>
    <display-name xml:lang="EN">JQuery Plugin Portlet</display-name>
    <portlet-class>org.gatein.portlet.amd.JQueryPluginPortlet</portlet-class>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>view</portlet-mode>
    </supports>
    <portlet-info>
      <title>Use jQuery plugin with the built-in jQuery</title>
    </portlet-info>
  </portlet>

  <portlet>
    <description xml:lang="EN">RequireJS Sample Portlet</description>
    <portlet-name>RequireJSPortlet</portlet-name>
    <display-name xml:lang="EN">RequireJS Portlet</display-name>
    <portlet-class>org.gatein.portlet.requirejs.RequireJSPortlet</portlet-class>

    <supports>
      <mime-type>text/html</mime-type>
    </supports>

    <portlet-info>
      <title>RequireJS Portlet</title>
      <short-title>RequireJS</short-title>
    </portlet-info>
  </portlet>

  <portlet>
    <description>CommonJS Sample Portlet</description>
    <portlet-name>CommonJSPortlet</portlet-name>
    <display-name>CommonJS Portlet</display-name>
    <portlet-class>org.gatein.portlet.commonjs.CommonJSPortlet</portlet-class>

    <supports>
      <mime-type>text/html</mime-type>
    </supports>

    <portlet-info>
      <title>CommonJS Sample Portlet</title>
      <short-title>CommonJS Portlet</short-title>
    </portlet-info>
  </portlet>
</portlet-app>