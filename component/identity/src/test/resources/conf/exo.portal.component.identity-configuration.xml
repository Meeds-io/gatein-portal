        </init-params>
      </component-plugin>
      <component-plugin>
        <name>MembershipUpdateListener</name>
        <set-method>addListenerPlugin</set-method>
        <type>org.exoplatform.services.organization.impl.MembershipUpdateListener</type>
      </component-plugin>
      <component-plugin>
        <name>MembershipUpdateListener</name>
